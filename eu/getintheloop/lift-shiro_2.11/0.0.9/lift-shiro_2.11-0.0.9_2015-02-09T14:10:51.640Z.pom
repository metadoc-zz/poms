<project 
xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <groupId>eu.getintheloop</groupId>
  <artifactId>lift-shiro_2.11</artifactId>
  <packaging>jar</packaging>
  <description>lift-shiro</description>
  <version>0.0.9</version>
  <licenses>
    <license>
      <name>Apache-2.0</name>
      <url>http://www.apache.org/licenses/LICENSE-2.0.html</url>
      <distribution>repo</distribution>
    </license>
  </licenses>
  <name>lift-shiro</name>
  <organization>
    <name>eu.getintheloop</name>
  </organization>
  <dependencies>
    <dependency>
      <groupId>org.scala-lang</groupId>
      <artifactId>scala-library</artifactId>
      <version>2.11.5</version>
    </dependency>
    <dependency>
      <groupId>org.apache.shiro</groupId>
      <artifactId>shiro-core</artifactId>
      <version>1.2.3</version>
    </dependency>
    <dependency>
      <groupId>org.apache.shiro</groupId>
      <artifactId>shiro-web</artifactId>
      <version>1.2.3</version>
    </dependency>
    <dependency>
      <groupId>commons-beanutils</groupId>
      <artifactId>commons-beanutils</artifactId>
      <version>1.9.2</version>
    </dependency>
    <dependency>
      <groupId>net.liftweb</groupId>
      <artifactId>lift-webkit_2.11</artifactId>
      <version>2.6</version>
      <scope>provided</scope>
    </dependency>
  </dependencies>
  <repositories>
    <repository>
      <id>CBCentralMirror</id>
      <name>CB Central Mirror</name>
      <url>http://repo.cloudbees.com/content/groups/public/</url>
      <layout>default</layout>
    </repository>
    <repository>
      <id>ShiroReleases</id>
      <name>Shiro Releases</name>
      <url>https://repository.apache.org/content/repositories/releases/</url>
      <layout>default</layout>
    </repository>
    <repository>
      <id>ShiroSnapshots</id>
      <name>Shiro Snapshots</name>
      <url>https://repository.apache.org/content/repositories/snapshots/</url>
      <layout>default</layout>
    </repository>
    <repository>
      <id>sonatyperepo</id>
      <name>sonatype.repo</name>
      <url>https://oss.sonatype.org/content/repositories/public/</url>
      <layout>default</layout>
    </repository>
    <repository>
      <id>BintrayResolvebtdmaven</id>
      <name>Bintray-Resolve-btd-maven</name>
      <url>http://dl.bintray.com/content/btd/maven/</url>
      <layout>default</layout>
    </repository>
  </repositories>
</project>


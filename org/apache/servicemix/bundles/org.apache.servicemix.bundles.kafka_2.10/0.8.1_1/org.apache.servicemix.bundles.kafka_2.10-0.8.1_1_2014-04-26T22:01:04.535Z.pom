<project 
xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://maven.apache.org/POM/4.0.0">
  <parent>
    <artifactId>bundles-pom</artifactId>
    <groupId>org.apache.servicemix.bundles</groupId>
    <version>11</version>
    <relativePath>../bundles-pom/pom.xml/pom.xml</relativePath>
  </parent>
  <modelVersion>4.0.0</modelVersion>
  <groupId>org.apache.servicemix.bundles</groupId>
  <artifactId>org.apache.servicemix.bundles.kafka_2.10</artifactId>
  <packaging>bundle</packaging>
  <name>Apache ServiceMix :: Bundles :: ${pkgArtifactId}</name>
  <version>0.8.1_1</version>
  <description>
    This OSGi bundle wraps ${pkgArtifactId} ${pkgVersion} jar file.
  </description>
  <scm>
    <connection>
      scm:svn:http://svn.apache.org/repos/asf/servicemix/smx4/bundles/tags/org.apache.servicemix.bundles.kafka_2.10-0.8.1_1
    </connection>
    <developerConnection>
      scm:svn:https://svn.apache.org/repos/asf/servicemix/smx4/bundles/tags/org.apache.servicemix.bundles.kafka_2.10-0.8.1_1
    </developerConnection>
    <url>
      http://svn.apache.org/viewvc/servicemix/smx4/bundles/tags/org.apache.servicemix.bundles.kafka_2.10-0.8.1_1
    </url>
  </scm>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-shade-plugin</artifactId>
        <executions>
          <execution>
            <phase>package</phase>
            <goals>
              <goal>shade</goal>
            </goals>
            <configuration>
              <artifactSet>
                <includes>
                  <include>${pkgGroupId}:${pkgArtifactId}</include>
                </includes>
              </artifactSet>
              <filters>
                <filter>
                  <artifact>${pkgGroupId}:${pkgArtifactId}</artifact>
                  <excludes>
                    <exclude>**/*</exclude>
                  </excludes>
                </filter>
              </filters>
              <promoteTransitiveDependencies>true</promoteTransitiveDependencies>
              <createDependencyReducedPom>true</createDependencyReducedPom>
            </configuration>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>
  <properties>
    <pkgVersion>0.8.1</pkgVersion>
    <servicemix.osgi.import.pkg>
      com.yammer.metrics*;version=&quot;[2.2,3)&quot;,
            javax.management*,
            joptsimple;version=&quot;[3.2,4)&quot;;resolution:=optional,
            org.I0Itec.zkclient*;resolution:=optional,
            org.apache.log4j*;resolution:=optional,
            org.apache.zookeeper*;version=&quot;[3.3,4)&quot;,
            org.xerial.snappy;resolution:=optional,
            scala*;version=&quot;[2,3)&quot;,
            sun.misc;resolution:=optional,
            sun.nio.ch;resolution:=optional
    </servicemix.osgi.import.pkg>
    <servicemix.osgi.export.pkg>kafka</servicemix.osgi.export.pkg>
    <pkgArtifactId>kafka_2.10</pkgArtifactId>
    <pkgGroupId>org.apache.kafka</pkgGroupId>
  </properties>
</project>


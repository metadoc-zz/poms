  <version>1.3.15</version>
  <licenses>
    <license>
      <name>Apache License, Version 2.0</name>
      <url>http://www.apache.org/licenses/LICENSE-2.0.txt</url>
      <distribution>repo</distribution>
    </license>
  </licenses>
  <name>Gerweck Utils</name>
  <inceptionYear>2012</inceptionYear>
  <organization>
    <name>org.gerweck.scala</name>
    <url>https://github.com/sarahgerweck/scala-utils</url>
  </organization>
  <scm>
    <url>https://github.com/sarahgerweck/scala-utils</url>
    <connection>scm:git:git@github.com:sarahgerweck/scala-utils.git</connection>
  </scm>
  <developers>
    <developer>
      <id>sarah</id>
      <name>Sarah Gerweck</name>
      <email>sarah.a180@gmail.com</email>
      <url>https://github.com/sarahgerweck</url>

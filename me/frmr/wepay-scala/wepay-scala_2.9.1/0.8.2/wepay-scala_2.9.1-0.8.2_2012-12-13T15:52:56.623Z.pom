      <version>2.5-SNAPSHOT</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>net.liftweb</groupId>
      <artifactId>lift-util_2.9.1</artifactId>
      <version>2.5-SNAPSHOT</version>
      <scope>compile</scope>

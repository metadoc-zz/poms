  <modelVersion>4.0.0</modelVersion>
  <groupId>me.frmr.wepay-scala</groupId>
  <artifactId>wepay-scala_2.9.1</artifactId>
  <packaging>jar</packaging>
  <description>A scala library for WePay payments processing.</description>
  <version>0.8.2</version>
  <name>wepay-scala</name>

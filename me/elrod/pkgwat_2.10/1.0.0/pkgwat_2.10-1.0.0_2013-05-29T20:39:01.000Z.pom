<project 
xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <groupId>me.elrod</groupId>
  <artifactId>pkgwat_2.10</artifactId>
  <packaging>jar</packaging>
  <description>
    A (rough) port of the pkgwat Fedora Packages API to Scala.
  </description>
  <url>https://github.com/CodeBlock/pkgwat-scala</url>
  <version>1.0.0</version>
  <licenses>
    <license>
      <name>Apache v2</name>
      <url>https://www.apache.org/licenses/LICENSE-2.0.txt</url>
      <distribution>repo</distribution>
    </license>
  </licenses>
  <name>pkgwat</name>
  <organization>
    <name>me.elrod</name>
    <url>https://github.com/CodeBlock/pkgwat-scala</url>
  </organization>
  <scm>
    <url>git@github.com:CodeBlock/pkgwat-scala.git</url>
    <connection>scm:git:git@github.com:CodeBlock/pkgwat-scala.git</connection>
  </scm>
  <developers>
    <developer>
      <id>relrod</id>
      <name>Ricky Elrod</name>
      <url>http://elrod.me/</url>
    </developer>
  </developers>
  <properties>

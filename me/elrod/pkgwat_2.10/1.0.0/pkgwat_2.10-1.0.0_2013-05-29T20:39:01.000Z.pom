      +e:scalaVersion:#@#:+2.10:#@#:+revision:#@#:+1.0.2:#@#:+module:#@#:+sbt-stats:#@#:+e:sbtVersion:#@#:+0.12:#@#:+organisation:#@#:+com.orrsella:#@#:+branch:#@#:+@#:NULL:#@:#@#:
    </extraDependencyAttributes>
  </properties>
  <dependencies>
    <dependency>
      <groupId>org.scala-lang</groupId>
      <artifactId>scala-library</artifactId>
      <version>2.10.1</version>

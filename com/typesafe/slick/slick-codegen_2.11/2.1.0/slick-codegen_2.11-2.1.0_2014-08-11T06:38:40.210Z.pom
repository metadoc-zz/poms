    </license>
  </licenses>
  <name>Slick-CodeGen</name>
  <inceptionYear>2008</inceptionYear>
  <organization>
    <name>Typesafe</name>
    <url>http://slick.typesafe.com</url>
  </organization>
  <developers>
    <developer>
      <id>szeiger</id>
      <name>Stefan Zeiger</name>
      <timezone>+1</timezone>
      <url>http://szeiger.de</url>
    </developer>
    <developer>
      <id>cvogt</id>
